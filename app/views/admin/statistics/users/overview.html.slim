h1 Статистика

canvas#myChart width="400" height="100"
javascript:
  $.get("#{admin_statistics_users_signups_path(format: :json)}").done(function(data){
    var canvas = document.getElementById("myChart");
    var ctx = canvas.getContext("2d");

    var myNewChart = new Chart(ctx , {
        type: "bar",
        data: data,
        options: {
          scales: {
            yAxes: [{
              ticks: {
                beginAtZero:true
              }
            }]
          }
        }
    });
  });
